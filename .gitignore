.gitignore

# Maven target directory
target/

# IntelliJ IDEA project files
.idea/
*.iml

# Logs
*.log

# OS-specific files
.DS_Store
Thumbs.db

# Build output
out/

# Temporary files
*.tmp
*.swp

# Java-specific
*.class

# Generated files
*.jar
*.war
*.ear

